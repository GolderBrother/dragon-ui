{
  "name": "dragon-ui",
  "version": "0.1.0-beta.4",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run deploy && npm run dev",
    "clean": "rm -rf lib && rm -rf dist",
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "pro": "rm -rf assets && webpack --config webpack.config.pro.js",
    "deploy": "rm -rf node_modules && npm install && npm run build",
    "lint": "eslint components examples --ext '.js,.jsx'",
    "build:lib": "NODE_ENV=production babel components --out-dir lib",
    "build:umd": "webpack --config webpack.config.umd.js",
    "build:umd:min": "webpack -p --config webpack.config.umd.min.js",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min"
  },
  "dependencies": {
    "classnames": "^2.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bundle-loader": "^0.5.0",
    "css-loader": "^0.15.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-za": "^1.0.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.13.3",
    "file-loader": "^1.1.11",
    "highlight.js": "^9.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^3.1.3",
    "react-router": "^3.2.1",
    "sass-loader": "^7.0.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "tags": [
    "react",
    "dragon"
  ],
  "keywords": [
    "react",
    "react-component",
    "dragon-ui",
    "dragon"
  ],
  "description": "UI for react.js",
  "author": "Jerome.Lin <16908091@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/JeromeLin/dragon-ui"
  },
  "bugs": {
    "url": "https://github.com/JeromeLin/dragon-ui/issues"
  }
}
